<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.image.pdf2reader" name="PDF to Reader" version="1.0.3.002" provider-name="YDRAGON">
  <requires>
    <import addon="xbmc.python" version="2.18.0"/>
    <import addon="script.module.pdf2image" version="1.4.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>image</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <language>en</language>
    <platform>all</platform>
    <summary lang="en">PDF Reader</summary>
    <description lang="en">Read your pdf's.</description>
    <platform>all</platform>
	<license>GPL 2.0</license>
  </extension>
</addon>

<addon id="repository.yellowdragon.ext" name="Yellow Dragon Repository Ext" version="18.4" provider-name="YLLOW_DRAGON">
    <requires>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="Yellow Dragon Repository Ext">
        <info compressed="false">https://raw.githubusercontent.com/nkvoronov/repository.yellowdragon.ext/leia/repo/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/nkvoronov/repository.yellowdragon.ext/leia/repo/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/nkvoronov/repository.yellowdragon.ext/leia/repo/</datadir>
        <hashes>false</hashes>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">YLLOW_DRAGON EXT addons repository for KODI Leia</summary>
        <summary lang="ru">Репозиторий плагинов YLLOW_DRAGON EXT для KODI Leia</summary>
        <description  lang="en">Download and install YLLOW_DRAGON EXT addons repository.</description>
        <description  lang="ru">Загружайте и устанавливайте плагины из репозитория YLLOW_DRAGON EXT.</description>
        <platform>all</platform>
        <assets>
            <icon>resources/icon.png</icon>
            <fanart>resources/fanart.jpg</fanart>
        </assets>
        <news></news>
    </extension>
</addon>

<addon id="script.module.pdf2image"
		name="PDF to Image"
		version="1.4.1.002"
		provider-name="YDRAGON">
	<requires>
		<import addon="xbmc.python" version="2.18.0" />
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary>convert PDF to PIL Image object.</summary>
		<description>A python module that wraps the pdftoppm utility to convert PDF to PIL Image object</description>
		<license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
		<platform>all</platform>
		<email>nkvoronov@gmail.com</email>
		<source>https://github.com/nkvoronov/script.module.pdf2image.git</source>
	</extension>
</addon>

